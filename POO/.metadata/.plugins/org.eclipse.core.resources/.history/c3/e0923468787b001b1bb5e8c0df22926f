package ExerciciosPOO;
import java.util.*;
public class Cliente {
	public String nome;
	public String cpf;
	public int idade;


	public Cliente (String nome) {
		Scanner teclado = new Scanner(System.in);
		System.out.println("Escreva seu nome: "); 
		nome = teclado.nextLine();
		System.out.printf("Seu nome é %s",nome);
	}
}